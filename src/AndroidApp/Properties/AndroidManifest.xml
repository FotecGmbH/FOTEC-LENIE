<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="at.dihost.lenie.dev"
          android:versionCode="17039616" android:versionName="1.4.1.0" android:installLocation="auto">
	<uses-sdk android:minSdkVersion="26" android:targetSdkVersion="33" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<application android:label="LENIE Dev" android:largeHeap="true" android:theme="@style/MainTheme"
	             android:icon="@drawable/icon" android:hardwareAccelerated="false">
		<!-- Standardordner für Fileaccess, zu ändern im genannten xml file. -->
		<provider android:name="androidx.core.content.FileProvider" android:authorities="${applicationId}.fileprovider"
		          android:exported="false" android:grantUriPermissions="true">
			<meta-data android:name="android.support.FILE_PROVIDER_PATHS" android:resource="@xml/file_paths"></meta-data>
		</provider>
		<!-- Default Icon für Toasts und falls vorhanden Pushes -->
		<meta-data android:name="com.google.firebase.messaging.default_notification_icon"
		           android:resource="@drawable/ic_notification" />
		<!-- Maps Api Key -->
		<meta-data android:name="com.google.android.geo.API_KEY" android:value="FOTEC:GoogleMapsKey" />
		<meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version" />
	</application>
</manifest>